{   "env":{
    "browser":true
    },
    "extends": "airbnb",
    // parser: "@babel/eslint-parser", (new one)
    "parser": "babel-eslint",
    // exclude JSX not allowed in files with extension '.js' error
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
      }
}